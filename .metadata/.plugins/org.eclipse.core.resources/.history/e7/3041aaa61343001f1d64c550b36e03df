package com.arrays;

public class Largest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n=0;
		if(n>0)
		{
			System.out.println("positive");
		}
		else if(n==0)
		{
			System.out.println("equal");
		}
		else
		{
			System.out.println("negative");
		}
	}

}
