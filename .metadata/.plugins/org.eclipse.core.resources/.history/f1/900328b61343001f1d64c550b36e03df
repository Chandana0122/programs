package com.arrays;

public class Largest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char ch='c';
		if((ch>='a'&&ch<='z')|| (ch>='A'&& ch<='Z'))
		{
			switch(ch)
			{
			case 'a':
			case 'e':
			case 'i':
			case 'o':
			case 'u':
			case 'A':
			case 'E':
			case 'I':
			case 'O':
			case 'U':
				System.out.println("vowels");
				break;
				default:
					System.out.println("consonanta");
			
			}
		}
		else
		{
			System.out.println("special characters");
		}
	}

}
