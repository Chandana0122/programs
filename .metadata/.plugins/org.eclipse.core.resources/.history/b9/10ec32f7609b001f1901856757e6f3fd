package com.kodnest;

public class Arr2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int age[] = new int[5];
		age[0] = 11;
		age[1] = 22;
		age[2] = 33;
		age[3] = 44;
		age[4] = 55;
		for (int i =0; i < age.length; i++) {
			System.out.println(age[i]);
		}

	}

}
