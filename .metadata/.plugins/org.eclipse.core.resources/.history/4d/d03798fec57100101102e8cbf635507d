package package1;

public class person1 {
	    private String name;
	    private float age;

	    // Constructor
	    public person1(String name, float age) {
	        this.name = name;
	        this.age = age;
	    }

	    // Getters and setters
	    public String getName() {
	        return name;
	    }

	    public void setName(String name) {
	        this.name = name;
	    }

	    public float getAge() {
	        return age;
	    }

	    public void setAge(float age) {
	        this.age = age;
	    }

	    @Override
	    public String toString() {
	        return "Person [name=" + name + ", age=" + age + "]";
	    }
	}



