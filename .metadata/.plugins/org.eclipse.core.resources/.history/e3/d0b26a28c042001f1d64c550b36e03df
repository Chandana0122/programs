package com.arrays;

public class Revorder {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] a= {1,2,3,4,5,-2,7,8};
		int small=a[0];
			for(int i=1;i<=a.length-1;i=i+1)
			{
				if(small>a[i]);
				{
					small=a[i];
				}
			}
			System.out.println(small);
		

	}

}
